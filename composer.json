{
    "name": "talav/media",
    "description": "Plan and plan feature functionality for SaaS applications",
    "keywords": ["symfony", "bundle", "subscription", "php"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Talav",
            "email": "support@talav.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "league/flysystem": "^3.0",
        "league/glide": "^2.2",
        "mark-gerarts/auto-mapper-plus": "^1.3",
        "symfony/http-foundation": "^6.0",
        "symfony/validator": "^6.0",
        "symfony/mime": "^6.0",
        "symfony/event-dispatcher": "^6.0",
        "symfony/messenger": "^6.0",
        "talav/resource": "@dev",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^6.0",
        "symfony/thanks": "^1.2",
        "webfactory/doctrine-orm-test-infrastructure": "^1.10",
        "fakerphp/faker": "^1.19",
        "league/flysystem-memory": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Talav\\Component\\Media\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Talav\\Component\\Media\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ]
}
